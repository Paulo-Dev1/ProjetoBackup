@model Mulher_Presente.Models.Parceiros

@{
    ViewData["Title"] = "Deletar Parceiro";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
<body>

<h3>Tem certeza que deseja deletar este parceiro?</h3>
<div>
    
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Especilidade)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Especilidade)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Telefone)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Telefone)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.email)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.email)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="id_parceiro" />
        <input type="submit" value="Excluir" class="btn btn-outline-danger" />
        <a asp-action="Index"><p class="h3">&#8592;</p></a>
    </form>
</div>
</body>
</html>
